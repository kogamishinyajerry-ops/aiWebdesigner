# ==========================================
# AI Designer - 环境变量配置模板
# ==========================================
# 复制此文件为 .env 并填入实际值
# 注意: 不要将 .env 文件提交到版本控制系统
# ==========================================

# === 应用配置 ===
APP_NAME=AI Designer
APP_ENV=development
DEBUG=true
SECRET_KEY=your-secret-key-change-this-in-production

# === 数据库配置 ===
DATABASE_URL=postgresql+asyncpg://postgres:postgres@localhost:5432/ai_designer
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=ai_designer
DB_POOL_SIZE=20
DB_MAX_OVERFLOW=10

# === Redis 配置 ===
REDIS_URL=redis://localhost:6379/0
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=
REDIS_ENABLED=True
REDIS_MAX_CONNECTIONS=20

# === AI 模型配置 ===
# Gemini API
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_MODEL=gemini-2.0-flash-exp
GEMINI_ENABLED=True

# 图像生成模型
IMAGE_MODEL_ID=black-forest-labs/FLUX.1-schnell
IMAGE_GENERATION_ENABLED=True
DEFAULT_IMAGE_SIZE=hero_medium
DEFAULT_IMAGE_STYLE=modern

# CLIP 美学模型
CLIP_MODEL_ID=laion/CLIP-ViT-L-14-DataComp.XL-s13B-b90k
CLIP_ENABLED=True

# === 向量数据库配置 ===
QDRANT_URL=http://localhost:6333
QDRANT_API_KEY=
QDRANT_ENABLED=False

# === CORS 配置 ===
CORS_ORIGINS=http://localhost:3000,http://localhost:8000

# === 速率限制配置 ===
RATE_LIMIT_ENABLED=True
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW=60

# === 缓存配置 ===
CACHE_ENABLED=true
CACHE_TTL=3600

# === 日志配置 ===
LOG_LEVEL=info
LOG_FILE=/var/log/ai-designer/app.log

# === 存储配置 ===
UPLOAD_DIR=./data/uploads
MAX_UPLOAD_SIZE=10485760

# === AWS S3 (可选) ===
# AWS_ACCESS_KEY_ID=
# AWS_SECRET_ACCESS_KEY=
# AWS_REGION=us-east-1
# AWS_BUCKET=
# AWS_ENABLED=false

# === 服务端口 ===
BACKEND_PORT=8000
FRONTEND_PORT=3000
HTTP_PORT=80
HTTPS_PORT=443

# === 前端 API URL ===
NEXT_PUBLIC_API_URL=http://localhost:8000
NEXT_PUBLIC_API_V1_URL=http://localhost:8000/api/v1
NEXT_PUBLIC_APP_NAME=AI Designer
NEXT_PUBLIC_APP_URL=http://localhost:3000

# === 监控和分析 (可选) ===
# SENTRY_DSN=
# GOOGLE_ANALYTICS_ID=
# ENABLE_ANALYTICS=false
